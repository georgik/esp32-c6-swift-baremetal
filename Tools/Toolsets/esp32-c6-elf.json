{
  "schemaVersion": "1.0",
  "swiftCompiler": {
    "extraCLIOptions": [
      "-enable-experimental-feature", "Embedded",
      "-Xfrontend", "-disable-stack-protector",
      "-use-ld=lld",
      "-nostartfiles",
      "-Xlinker", "-nostdlib",
      "-Xclang-linker", "-nostdlib",
      "-Xlinker", "-static",
      "-wmo",
      "-O"
    ]
  },
  "linker": {
    "extraCLIOptions": [
      "-T", "Sources/Support/ld/esp32c6/linkall.x",
      "-e", "_start",
      "-L", "Sources/Support/ld/esp32c6",
      "-L", "Sources/Support/ld/sections",
      "-L", "Sources/Support/ld/riscv"
    ]
  }
}