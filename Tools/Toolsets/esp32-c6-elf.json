{
  "schemaVersion": "1.0",
  "swiftCompiler": {
    "extraCLIOptions": [
      "-enable-experimental-feature", "Embedded",
      "-Xfrontend", "-mergeable-symbols",
      "-Xfrontend", "-disable-stack-protector",
      "-use-ld=lld",
      "-nostartfiles",
      "-Xlinker", "-nostdlib",
      "-Xlinker", "-static",
      "-Xclang-linker", "-nostdlib"
    ]
  },
  "linker": {
    "extraCLIOptions": [
      "-T", "Sources/Support/linkerscript.ld",
      "-e", "_start",
      "-L", "Sources/Support/ld/esp32c6",
      "-L", "Sources/Support/ld/sections",
      "-L", "Sources/Support/ld/riscv"
    ]
  }
}